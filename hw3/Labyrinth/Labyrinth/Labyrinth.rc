/////////////////////////////////////////////////////////////////////////////
//
// Menu
//

IDR_MAINFRAME MENU
BEGIN
    MENUITEM SEPARATOR
    MENUITEM SEPARATOR
    POPUP "文件(&F)"
    BEGIN
        MENUITEM "新建(&N)\tCtrl+N",              ID_FILE_NEW
        MENUITEM "打开(&O)...\tCtrl+O",           ID_FILE_OPEN
        MENUITEM "保存(&S)\tCtrl+S",              ID_FILE_SAVE
        MENUITEM "另存为(&A)...",                  ID_FILE_SAVE_AS
        MENUITEM SEPARATOR
        MENUITEM "最近的文件",                       ID_FILE_MRU_FILE1, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "退出(&X)",                      ID_APP_EXIT
    END
    POPUP "搜索(&S)"
    BEGIN
        MENUITEM "开始搜索",                        ID_SEARCH_START
        POPUP "搜索方式"
        BEGIN
            MENUITEM "宽度优先",                        ID_WIDTH_PRIOR
            MENUITEM "深度优先",                        ID_DEPTH_PRIOR
            MENUITEM "改进型深度优先",                     ID_DEPTH_PRIOR2
            MENUITEM "A*算法",                        ID_ASTAR_SEARCH
        END
        MENUITEM "搜索效率分析",                      ID_SEARCH_ANALYSIS
    END
    POPUP "地图(&M)"
    BEGIN
        POPUP "生成地图"
        BEGIN
            MENUITEM "创建新地图",                       ID_MAP_NEW
            MENUITEM "随机生成",                        ID_MAP_RAND
            MENUITEM "随机更新",                        ID_MAP_REFRESH
            MENUITEM "预设地图",                        ID_MAP_DEFAULT
            MENUITEM "导入地图",                        ID_MAP_INPUT
        END
        POPUP "编辑地图"
        BEGIN
            MENUITEM "设定尺寸",                        ID_MAP_RESIZE
            MENUITEM "设定出入口",                       ID_MAP_ENTRY
            MENUITEM "断路比例",                        ID_MAP_RATIO
        END
    END
    POPUP "视图(&V)"
    BEGIN
        POPUP "工具栏和停靠窗口(&T)"
        BEGIN
            MENUITEM "<占位符>",                       ID_VIEW_TOOLBAR
        END
        MENUITEM "状态栏(&S)",                     ID_VIEW_STATUS_BAR
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于 Labyrinth(&A)...",         ID_APP_ABOUT
    END
END

